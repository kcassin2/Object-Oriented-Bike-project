import java.util.ArrayList;

public class OfficeManager extends Account{
  public Inventory examinePartByName(String partName) {
    	Inventory i = MainWareHouse.findPartByName(partName);
    	return i;
  }
  public Inventory examinePartByNumber(int partNumber) {
    	Inventory i = MainWareHouse.findPartByNumber(partNumber);
    	return i;
  }
  public String viewNotification() {
  	minimum = 10;
	String s;
  	for (Inventory i : whDB) {
		if (i.getCount()<=minimum){
			s += i;
		}
	}
	return s;
  }
  public String queryWarehouse() {
  	String s;
  	for (Inventory i : whDB) {
  		s += i.SORT_BY_COUNT;
	}
	return s;
  }
  public void orderPart(String partName, int quantity) {
  	Inventory i = MainWareHouse.findPartByName(partName);
	addinventory(i.getBp,quantity);
  }
  public String viewInvoiceC(String client) {
  	
  }
  public String viewInvoiceA(String associateName) {
  	
  }
  public double generatePay(String associateName, Date sDate, Date eDate) {
  	
  }
}
