<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <tabs>
      <Tab text="Examine Part by Name">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <Button fx:id="OMEBPNameButton" layoutX="274.0" layoutY="113.0" mnemonicParsing="false" onAction="#getOMEBPNameButton" text="Search" />
                  <Label fx:id="OMEBPNameLabel" layoutX="60.0" layoutY="74.0" text="Enter the Bike Part name:" />
                  <TextField fx:id="OMEBPNameTL" layoutX="200.0" layoutY="70.0" onAction="#getOMEBPNameTL" prefHeight="26.0" prefWidth="300.0" />
                  <TextArea fx:id="OMBPNameTA" layoutX="60.0" layoutY="155.0" prefHeight="200.0" prefWidth="480.0" />
               </children>
            </AnchorPane>
        </content>
      </Tab>
    <Tab text="Examine Part by Number">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <TextArea fx:id="OMEBPNumberTA" layoutX="60.0" layoutY="155.0" prefHeight="200.0" prefWidth="480.0" />
                  <TextField fx:id="OMEBPNumberTL" layoutX="200.0" layoutY="70.0" onAction="#getOMEBPNumberTL" prefWidth="300.0" />
                  <Label fx:id="OMEBPNumberLabel" layoutX="60.0" layoutY="74.0" text="Enter Bike Part Number: " />
                  <Button fx:id="OMEBPNumberButton" layoutX="274.0" layoutY="113.0" mnemonicParsing="false" onAction="#getOMEBPNumberButton" text="Search" />
               </children>
            </AnchorPane>
      </content>
    </Tab>
      <Tab text="Order Parts">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
      <Tab text="View Sales Invoices">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
      <Tab text="Sales Commission">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
  </tabs>
</TabPane>
