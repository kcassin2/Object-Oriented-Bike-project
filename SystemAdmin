import java.util.ArrayList;
public class SystemAdmin extends Account{

	public OfficeManager addOfficeManager(String firstName, String lastName, String email, String uName, String password){
	  	OfficeManager om = Account.addAccount(firstName, lastName, email, uName, password);
    		return om;
	}

	public WareHouseManager addWareHouseManager(String firstName, String lastName, String email, String uName, String password) {	
    		WareHouseManager wm = Account.addAccount(firstName, lastName, email, uName, password);
    		return wm;
	}
  
	public SalesAssociate addSalesAssociate(String firstName, String lastName, String email, String uName, String password, String vanName) {
    		SalesAssociate sa = Account.addAccount(firstName, lastName, email, uName, password);
    		Van SaVan= New Van();
    		SaVan.setAssociate(fName+lName);
    		return sa;
	}
}
